CREATE VIEW localized_de_com_logali_sales_SelProduct3 AS SELECT
  ProductReview_1.Rating,
  Products_0.name,
  sum(Products_0.Price) AS totalPrice
FROM (localized_de_com_logali_materials_Products AS Products_0 LEFT JOIN localized_de_com_logali_materials_ProductReview AS ProductReview_1 ON Products_0.ID = ProductReview_1.ID)
GROUP BY ProductReview_1.Rating, Products_0.name
ORDER BY Rating;

CREATE VIEW localized_fr_com_logali_sales_SelProduct3 AS SELECT
  ProductReview_1.Rating,
  Products_0.name,
  sum(Products_0.Price) AS totalPrice
FROM (localized_fr_com_logali_materials_Products AS Products_0 LEFT JOIN localized_fr_com_logali_materials_ProductReview AS ProductReview_1 ON Products_0.ID = ProductReview_1.ID)
GROUP BY ProductReview_1.Rating, Products_0.name
ORDER BY Rating;

